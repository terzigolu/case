# API Configuration
API_URL=https://fakestoreapi.com
API_TIMEOUT=30000

# App Configuration
APP_NAME=GetCase
APP_ENV=development

# Feature Flags
ENABLE_ANALYTICS=false
ENABLE_CRASH_REPORTING=false

# Cache Configuration
CACHE_TIMEOUT=3600
MAX_CACHE_SIZE=50

# Image Configuration
IMAGE_CACHE_SIZE=100
MAX_IMAGE_WIDTH=1024
MAX_IMAGE_HEIGHT=1024

# Debug Configuration
ENABLE_DEBUG_LOGGING=true
ENABLE_NETWORK_LOGGING=true

# Not: Bu dosyayı .env olarak kopyalayın ve değerleri kendi ortamınıza göre güncelleyin.
# Önemli: Gerçek API anahtarları veya hassas bilgileri asla bu dosyaya eklemeyin. 